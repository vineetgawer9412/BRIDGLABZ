public class MultiTables {
    public static void main(String[] args) {
        int[] results = new int[40]; // 4 tables * 10
        int index = 0;

        for (int n = 6; n <= 9; n++) {
            for (int i = 1; i <= 10; i++) {
                results[index++] = n * i;
                System.out.println(n + " * " + i + " = " + (n * i));
            }
            System.out.println();
        }
    }
}
